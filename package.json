{
  "name": "@conio/conio-frontend-boilerplate",
  "version": "0.1.0",
  "description": "Frontend starter kit: offline-first responsive PWA",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test npm run lint",
    "lint": "tslint -c ./tslint.json './src/**/*.{ts,tsx}'",
    "build": "NODE_ENV=development webpack --config ./webpack.config.js",
    "watch": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "server": "NODE_ENV=development nodemon --config ./nodemon.mock.json",
    "start": "concurrently \"npm run watch\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/squadrone/conio-frontend-boilerplate.git"
  },
  "keywords": [
    "javascript",
    "react",
    "redux",
    "pwa",
    "serviceworker"
  ],
  "author": "Conio <info@conio.com>",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/squadrone/conio-frontend-boilerplate#readme",
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/cookie-parser": "^1.4.1",
    "@types/errorhandler": "0.0.32",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "browser-sync": "^2.23.5",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chalk": "^2.3.0",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.11",
    "pug": "^2.0.0-rc.4",
    "source-map-loader": "^0.2.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
